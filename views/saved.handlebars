<div class="container">

    <div class="col-md-12 jumbotron">
        <div class="text-center">
            <h1> Saved Articles</h1>
        </div>
    </div>
  <div id="articles">
{{#each articles}}
<div>
    <p data-id="{{_id}}">{{title}}<br>{{link}}<br>{{summary}}</p>
</div>
{{/each}}

  </div><div class="col-md-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                   <br>
                </div>
    </div>
</div>
  <div id="notes"></div>
  </div>
  <form>
                        <button type="button" class="btn btn-success addNote" data-toggle="modal" data-target="#noteModal">View/Add Notes</button>
                        <button type="button" id= "deletebtn" class="btn btn-danger delete">Delete</button>
                    </div>
                    
                </div>
                {{#if noArticles}}
        <div class="panel panel-default">
            <div class="panel-body">
                <h4>You have no saved articles.</h4>
            </div>
            {{/if}}
        </div>
    </div>
</div>

      <script src="saved.js"></script>
